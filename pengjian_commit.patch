+package com.pazz.entity;
+
+/**
+ * Created by Administrator on 2017-4-8.
+ */
+public class Person {
+
+    private int pid;
+    private String pname;
+    private int page;
+
+    public Person() {
+    }
+
+    public Person(int pid, String pname, int page) {
+        this.pid = pid;
+        this.pname = pname;
+        this.page = page;
+    }
+
+    public int getPid() {
+        return pid;
+    }
+
+    public void setPid(int pid) {
+        this.pid = pid;
+    }
+
+    public String getPname() {
+        return pname;
+    }
+
+    public void setPname(String pname) {
+        this.pname = pname;
+    }
+
+    public int getPage() {
+        return page;
+    }
+
+    public void setPage(int page) {
+        this.page = page;
+    }
+
+    @Override
+    public String toString() {
+        return "Person{" +
+                "pid=" + pid +
+                ", pname='" + pname + '\'' +
+                ", page=" + page +
+                '}';
+    }
+}
